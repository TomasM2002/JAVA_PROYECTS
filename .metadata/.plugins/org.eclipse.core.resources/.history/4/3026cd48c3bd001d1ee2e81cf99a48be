package Punto01;

public class puntoC {

	public static void main(String[] args) {
		int numeroInicio = 5;
		int numeroFinal = 14;
		boolean esImpar = true;
		int i = numeroInicio;
		
		if(esImpar){
			while (i <= numeroFinal) {
				if (i % 2 != 0) {
					System.out.println(i);
				}
				i = i +1;
			}
				
		} else {
			while (i <= numeroFinal) {
				if (i % 2 == 0) {
					System.out.println(i);
				}
				i = i +1;
			}
		}
			
		
			
	}

}
