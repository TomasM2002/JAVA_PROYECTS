package Punto01.copy;

public class puntoB {

	public static void main(String[] args) {
		int numeroInicio = 5;
		int numeroFinal = 14;
		
		while (numeroInicio <= numeroFinal){
			if (numeroInicio % 2 == 0) {
				System.out.println( numeroInicio );
			}
			numeroInicio = numeroInicio + 1 ;
			
		}
		
		
		
	}

}
